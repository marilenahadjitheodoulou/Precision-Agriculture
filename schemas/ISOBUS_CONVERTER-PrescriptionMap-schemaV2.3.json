		"Task": {
			"type": "array",
			"uniqueItems": true,
			"items": [
				{
					"type": "object",
					"properties": {
						"TaskId": {
							"type": "string",
							"minLength": 4,
							"maxLength": 14,
							"pattern": "^(TSK|TSK-)[0-9]{1,11}"
						},
						"TaskDesignator": {
							"type": "string",
							"maxLength": 32
						},
						"CustomerIdRef": {
							"type": "string",
							"minLength": 4,
							"maxLength": 14,
							"pattern": "^(CTR|CTR-)[0-9]{1,11}"
						},
						"FarmIdRef": {
							"type": "string",
							"minLength": 4,
							"maxLength": 14,
							"pattern": "^(FRM|FRM-)[0-9]{1,11}"
						},
						"PartfieldIdRef": {
							"type": "string",
							"minLength": 4,
							"maxLength": 14,
							"pattern": "^(PFD|PFD-)[0-9]{1,11}"
						},
						"ResponsibleWorkerIdRef":{
							"type": "string",
							"minLength": 4,
							"maxLength": 14,
							"pattern": "^(WKR|WKR-)[0-9]{1,11}"
						},
						"DefaultTreatmentZoneCode":{
							"type":"integer",
							"minimum":0,
							"maximum":254
						},
						"PositionLostTreatmentZoneCode":{
							"type":"integer",
							"minimum":0,
							"maximum":254
						},
						"OutOfFieldTreatmentZoneCode":{
							"type":"integer",
							"minimum":0,
							"maximum":254
						},
						"TaskStatus":{
							"type":"integer",
							"description":"Status of the task, possible values:\n1 = planned\n2 = running\n3 = paused\n4 = completed\n5 = template\n6 = canceled",
							"minimum":1,
							"maximum":6
						},
						"TreatmentZone":{
							"type":"array",
							"description": "Inside a task, treatment zones can be referenced. Each TreatmentZone has a special meaning concerning the included ProcessDataVariables. The values of those ProcessDataVariables are to be applied globally to the whole task.",
							"items": {
								"type":"object",
								"uniqueItems": true,
								"properties": {
									"TreatmentZoneCode":{
										"type":"integer",
										"minimum": 0,
										"maximum": 254
									},
									"TreatmentZoneDesignator":{
										"type": "string",
										"maxLength": 32
									},
									"TreatmentZoneColour":{
										"type":"string",
										"minLength":8,
										"maxLength":8,
										"pattern": "^(0x)([a-fA-F0-9]{2})$+"
									},
									"Polygons": {
										"type":"array",
										"items": {
										"ref": "#/definitions/Polygon"
									}
									},
									"ProcessDataVariable":{
										"type": "array",
										"items": {
											"type":"object",
											"properties": {
												"ProcessDataValue": {
													"type":"number"
												},
												"UoM": {
													"type": "string",
													"maxLength": 32
												},
												"ProductIdRef":{
													"type": "string",
													"minLength": 4,
													"maxLength": 14,
													"pattern": "^(PDT|PDT-)[0-9]{1,11}"
												}
											},
											"additionalProperties": false,
											"required":[
											"ProcessDataValue",
											"UoM",
											"ProductIdRef"
											]
										}
									}
								},
								"additionalProperties": false,
								"required":[
								"TreatmentZoneCode"
								]
							}
						},
						"OperTechPractice":{
							"type":"object",
							"properties": {
								"CulturalPracticeIdRef":{
									"type": "string",
									"minLength": 4,
									"maxLength": 14,
									"pattern": "^(CPC|CPC-)[0-9]{1,11}"
								},
								"OperationTechniqueIdRef":{
									"type": "string",
									"minLength": 4,
									"maxLength": 14,
									"pattern": "^(OTQ|OTQ-)[0-9]{1,11}"
								}
							},
							"additionalProperties": false,
							"required":[
								"CulturalPracticeIdRef"
							]
						},
						"ProductAllocation":{
							"type":"object",
							"properties": {
								"ProductIdRef":{
									"type": "string",
									"minLength": 4,
									"maxLength": 14,
									"pattern": "^(PDT|PDT-)[0-9]{1,11}"
								}
							},
							"additionalProperties": true,
							"required":[
								"ProductIdRef"
							]
						},
						"Grid": {
							"type":"object",
							"properties": {
								"GridMinimumNorthPosition":{
									"type":"number",
									"description": "Minimum north position of the grid WGS84",
									"minimum": -90.0,
									"maximum": 90.0
								},
								"GridMinimumEastPosition":{
									"type":"number",
									"description": "Minimum east position of the grid WGS84",
									"minimum": -180.0,
									"maximum": 180.0
								},
								"GridCellNorthSize":{
									"type":"number",
									"description": "North direction gridcell size WGS84",
									"minimum": 0,
									"maximum": 1.0
								},
								"GridCellEastSize":{
									"type":"number",
									"description": "East direction gridcell size WGS84",
									"minimum": 0,
									"maximum": 1.0
								},
								"GridMaximumColumn":{
									"type":"integer",
									"description": "Number of the gridcells in east direction",
									"minimum": 0,
									"maximum": 2147483647
								},
								"GridMaximumRow":{
									"type":"integer",
									"description": "Number of the gridcells in north direction",
									"minimum": 0,
									"maximum": 2147483647
								},
								"GridCell":{
									"type":"array",
									"description": "The grid cells of a grid contain a reference to a TreatmentZone or a process data variable value",
									"items": {
										"type":"integer"
									}
								}
							},
							"additionalProperties": true,
							"required": [
								"GridMinimumNorthPosition",
								"GridMinimumEastPosition",
								"GridCellNorthSize",
								"GridCellEastSize",
								"GridMaximumColumn",
								"GridMaximumRow",
								"GridCell"
							]
						}
					},
					"additionalProperties": true,
					"required": [
						"TaskId",
						"TaskStatus"
					]
				}
			],
			"additionalItems": false
		},
