{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"definitions": {
		"Polygon": {
			"type": "object",
			"properties": {
				"PolygonType": {
					"type": "integer",
					"minimum": 1,
					"maximum": 12
				},
				"PoligonDesignator": {
					"type": "string",
					"maxLength": 32
				},
				"PoligonArea": {
					"type": "integer",
					"minimum": 0,
					"maximum": 4294967294
				},
				"LineString" : {
					"type":"object",
					"properties": {
						"LineStringType" : {
							"type":"integer",
							"description": "Type of the LineString,\npossible values:\n1 = PolygonExterior\n2 = PolygonInterior\n3 = TramLine\n4 = SamplingRoute\n5 = GuidancePattern\n6 = Drainage\n7 = Fence\n8 = Flag\n9 = Obstacle, any kind of\nobstacle, e.g. used to generate\na warning when approaching\nan obstacle",
							"minimum": 1,
							"maximum": 9
						},
						"LineStringDesignator" : {
							"type":"string",
							"maxLength":32
						},
						"LineStringWidth":{
							"type":"integer",
							"minimum": 0,
							"maximum": 4294967294
						},
						"LineStringLength":{
							"type":"integer",
							"minimum": 0,
							"maximum": 4294967294
						},
						"LineStringColour":{
							"type":"string",
							"minLength":8,
							"maxLength":8,
							"pattern": "^(0x)([a-fA-F0-9]{2})$+"
						},
						"LineStringIdRef": {
							"type": "string",
							"minLength": 4,
							"maxLength": 14,
							"pattern": "^(CTR|CTR-)[0-9]{1,11}"
						},
						"Point":{
							"type":"array",
							"items": [
								{
									"type":"object",
									"properties": {
										"PointType":{
											"type":"integer",
											"description": "Defines the meaning of this point:\n1 = Flag\n2 = other\n3 = Field Access, entry point of a fielda\n4 = Storage, location where product is stored on a field\n5 = Obstacle, any kind of obstacle, e.g. used to generate warnings when approaching a obstacle\n6 = Guidance Reference A, start point of a guidance line, e.g the A point in an A-B linea\n7 = Guidance Reference B, end point of a guidance line, e.g. the B point in an A-B linea\n8 = Guidance Reference Center, centre point of a guidance pivot pattern\n9 = Guidance Point, guidance related point that is neither an A, B or Center reference pointa\n10 = Partfield Reference Point, location in e.g. a Partfield that identifies the field for administrative purposes\n11 = Homebase",
											"minimum": 1,
											"maximum": 11
										},
										"PointDesignator": {
											"type":"string",
											"maxLength":32
										},
										"PointNorth" : {
											"type":"number",
											"description": "GPS position north WGS84",
											"minimum": -90.0,
											"maximum": 90.0
										},
										"PointEast":{
											"type":"number",
											"description": "GPS position east WGS84",
											"minimum": -180.0,
											"maximum": 180.0
										},
										"PointUp": {
											"type":"integer",
											"description": "GPS position altitude WGS84 ellipsoid in mm",
											"minimum": -2147483649,
											"maximum": 2147483647
										},
										"PointColour": {
											"type":"string",
											"minLength":8,
											"maxLength":8,
											"pattern": "^(0x)([a-fA-F0-9]{2})$+"
										},
										"PointId":{
											"type": "string",
											"minLength": 4,
											"maxLength": 14,
											"pattern": "^(PTN|PTN-)[0-9]{1,11}"
										},
										"PointHorizontalAccuracy":{
											"type":"number",
											"minimum": 0,
											"maximum": 65.0,
											"description": "Point horizontal estimated accuracy (RMS error) in m"
										},
										"PointVerticalAccuracy":{
											"type":"number",
											"minimum": 0,
											"maximum": 65.0,
											"description": "Point horizontal estimated accuracy (RMS error) in m"
										}
									},
									"additionalProperties": false,
									"required": [
										"PointNorth",
										"PointEast",
										"PointType"
									]
								}
							]
						}
					},
					"additionalProperties": false,
					"required": [
					"LineStringType",
					"Point"
					]
				}
			},
			"additionalProperties": false,
			"required": [
			"PolygonType"
			]
		}
	},
	"type": "object",
	"properties": {
		"VersionMajor":{
			"type": "integer",
			"minimum": 0,
			"maximum": 99
		},
		"VersionMinor":{
			"type": "integer",
			"minimum": 0,
			"maximum": 99
		},
