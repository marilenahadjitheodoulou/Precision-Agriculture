					"type": "object",
					"properties": {
						"CulturalPracticeId": {
							"type": "string" ,
							"minLength": 4,
							"maxLength": 14,
							"pattern": "^(CPC|CPC-)[0-9]{1,11}"
						},
						"CulturalPracticeDesignator": {
							"type": "string",
							"maxLength": 32
						},
						"OperationTechniqueReference": {
							"type": "array",
							"uniqueItems": true,
							"items": [
								{
									"type":"object",
									"properties":{
										"OperationTechniqueIdRef":{
											"type": "string" ,
											"minLength": 4,
											"maxLength": 14,
											"pattern": "^(OTQ|OTQ-)[0-9]{1,11}"
